lc;</span><br><span class="line"></span><br><span class="line">%% Gray-scale image</span><br><span class="line">Cameraman = imread(&apos;cameraman.tif&apos;);</span><br><span class="line">Cameraman = medfilt2(Cameraman);</span><br><span class="line">Cameraman2=Cameraman/255;</span><br><span class="line"></span><br><span class="line">%% SimpleDerivation</span><br><span class="line">fopgrx = -1:1;</span><br><span class="line">fopgry =fopgrx&apos;;</span><br><span class="line">Grxim=myCorrelation(Cameraman2,fopgrx);</span><br><span class="line">Gryim=myCorrelation(Cameraman2,fopgry);</span><br><span class="line">% Grxim=double(imfilter(Cameraman2,fopgrx));</span><br><span class="line">% Gryim=double(imfilter(Cameraman2,fopgry));</span><br><span class="line">MoGxy=sqrt(Grxim.^2+Gryim.^2);</span><br><span class="line">v1=0.5;</span><br><span class="line">[row1 col1]=size(MoGxy);</span><br><span class="line">for i=1:row1</span><br><span class="line">    for j=1:col1</span><br><span class="line">        if MoGxy(i,j)&gt;v1</span><br><span class="line">           fimedge1(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge1(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">v2=1.5;</span><br><span class="line">for i=1:row1</span><br><span class="line">    for j=1:col1</span><br><span class="line">        if MoGxy(i,j)&gt;v2</span><br><span class="line">           fimedge2(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge2(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">figure;</span><br><span class="line">subplot(2,3,1);</span><br><span class="line">imshow(Cameraman);</span><br><span class="line">title (&apos;original&apos;);</span><br><span class="line">subplot(2,3,2);</span><br><span class="line">imshow(Grxim);</span><br><span class="line">title (&apos;Grxim&apos;);</span><br><span class="line">subplot(2,3,3);</span><br><span class="line">imshow(Gryim);</span><br><span class="line">title (&apos;Gryim&apos;);</span><br><span class="line">subplot(2,3,4);</span><br><span class="line">imshow(MoGxy);</span><br><span class="line">title (&apos;MoGxy&apos;);</span><br><span class="line">subplot(2,3,5);</span><br><span class="line">imshow(fimedge1);</span><br><span class="line">title (&apos;threshold=0.5&apos;);</span><br><span class="line">subplot(2,3,6);</span><br><span class="line">imshow(fimedge2);</span><br><span class="line">title (&apos;threshold=1.5&apos;);</span><br><span class="line">%% Prewitt</span><br><span class="line">prewitt=[-1 0 1;-1 0 1;-1 0 1];</span><br><span class="line">prewittxim=double(imfilter(Cameraman2,prewitt));</span><br><span class="line">prewittyim=double(imfilter(Cameraman2,prewitt&apos;));</span><br><span class="line">Moprewitt=sqrt(prewittxim.^2+prewittyim.^2);</span><br><span class="line">v1=0.5;</span><br><span class="line">[row2 col2]=size(Moprewitt);</span><br><span class="line">for i=1:row2</span><br><span class="line">    for j=1:col2</span><br><span class="line">        if Moprewitt(i,j)&gt;v1</span><br><span class="line">           fimedge3(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge3(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">v2=1.5;</span><br><span class="line">for i=1:row2</span><br><span class="line">    for j=1:col2</span><br><span class="line">        if Moprewitt(i,j)&gt;v2</span><br><span class="line">           fimedge4(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge4(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">figure;</span><br><span class="line">subplot(2,3,1);</span><br><span class="line">imshow(Cameraman);</span><br><span class="line">title (&apos;original&apos;);</span><br><span class="line">subplot(2,3,2);</span><br><span class="line">imshow(prewittxim);</span><br><span class="line">title (&apos;prewittxim&apos;);</span><br><span class="line">subplot(2,3,3);</span><br><span class="line">imshow(prewittyim);</span><br><span class="line">title (&apos;prewittyim&apos;);</span><br><span class="line">subplot(2,3,4);</span><br><span class="line">imshow(Moprewitt);</span><br><span class="line">title (&apos;Moprewitt&apos;);</span><br><span class="line">subplot(2,3,5);</span><br><span class="line">imshow(fimedge3);</span><br><span class="line">title (&apos;threshold=0.5&apos;);</span><br><span class="line">subplot(2,3,6);</span><br><span class="line">imshow(fimedge4);</span><br><span class="line">title (&apos;threshold=1.5&apos;);</span><br><span class="line">%% Sobel</span><br><span class="line">Sobel=[-1 0 1;-2 0 2;-1 0 1];</span><br><span class="line">Sobelxim=double(imfilter(Cameraman2,Sobel));</span><br><span class="line">Sobelyim=double(imfilter(Cameraman2,Sobel&apos;));</span><br><span class="line">MoSobel=sqrt(Sobelxim.^2+Sobelyim.^2);</span><br><span class="line">v1=0.5;</span><br><span class="line">[row3 col3]=size(MoSobel);</span><br><span class="line">for i=1:row3</span><br><span class="line">    for j=1:col3</span><br><span class="line">        if MoSobel(i,j)&gt;v1</span><br><span class="line">           fimedge5(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge5(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">v3=1.2;</span><br><span class="line">for i=1:row3</span><br><span class="line">    for j=1:col3</span><br><span class="line">        if MoSobel(i,j)&gt;v3</span><br><span class="line">           fimedge6(i,j)=1;</span><br><span class="line">        else</span><br><span class="line">           fimedge6(i,j)=0;</span><br><span class="line">        end</span><br><span class="line">    end</span><br><span class="line">end</span><br><span class="line">figure;</span><br><span class="line">subplot(2,3,1);</span><br><span class="line">imshow(Cameraman);</span><br><span class="line">title (&apos;original&apos;);</span><br><span class="line">subplot(2,3,2);</span><br><span class="line">imshow(Sobelxim);</span><br><span class="line">title (&apos;Sobelxim&apos;);</span><br><span class="line">subplot(2,3,3);</span><br><span class="line">imshow(Sobelyim);</span><br><span class="line">title (&apos;Sobelyim&apos;);</span><br><span class="line">subplot(2,3,4);</span><br><span class="line">imshow(MoSobel);</span><br><span class="line">title (&apos;MoSobel&apos;);</span><br><span class="line">subplot(2,3,5);</span><br><span class="line">imshow(fimedge5);</span><br><span class="line">title (&apos;threshold=0.5&apos;);</span><br><span class="line">subplot(2,3,6);</span><br><span class="line">imshow(fimedge6);</span><br><span class="line">title (&apos;threshold=1.2&apos;);</span><br><span class="line">%% edge</span><br><span class="line">figure;</span><br><span class="line">subplot(2,2,1);</span><br><span class="line">imshow(Cameraman);</span><br><span class="line">title(&apos;original&apos;);</span><br><span class="line">BWPrewitt = edge(Cameraman,&apos;Prewitt&apos;);</span><br><span class="line">subplot(2,2,2);</span><br><span class="line">imshow(BWPrewitt);</span><br><span class="line">title(&apos;Prewitt&apos;);</span><br><span class="line">BWSobel = edge(Cameraman,&apos;Sobel&apos;);</span><br><span class="line">subplot(2,2,3);</span><br><span class="line">imshow(BWSobel);</span><br><span class="line">title(&apos;Sobel&apos;);</span><br><span class="line">BWCanny = edge(Cameraman,&apos;Canny&apos;);</span><br><span class="line">subplot(2,2,4);</span><br><span class="line">i